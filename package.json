{
  "name": "react-server-example-tsx",
  "version": "7.0.0",
  "description": "Boilerplate for isomorphic web app with React server-side rendering in TypeScript.",
  "main": "dist/server.js",
  "repository": "styfle/react-server-example-tsx",
  "scripts": {
    "start": "node dist/server.js",
    "build": "webpack --mode development",
    "now-build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "prettier": "prettier \"./src/**/*.ts\" \"./src/**/*.tsx\" --write",
    "test": "tsc --outDir . --sourceMap false && tape tests/**/*.js",
    "release": "npm version patch && git push --follow-tags && npm publish",
    "deploy": "now --public && now alias"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "dot": "^1.1.2",
    "express-ws": "^4.0.0",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-component-passport": "^3.9.0",
    "loopback-connector-postgresql": "^3.3.2",
    "passport-google-oauth": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "showdown": "^1.8.7",
    "strong-error-handler": "^2.0.0",
    "ws": "latest"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/tape": "^4.2.32",
    "@types/webpack": "^4.4.10",
    "prettier": "1.16.4",
    "source-map-loader": "^0.2.4",
    "tape": "^4.9.1",
    "ts-loader": "^5.2.1",
    "typescript": "^3.0.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "@babel/preset-stage-0": "^7.0.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-loopback": "^8.0.0",
    "eslint-config-standard": "^12.0.0",
    "nsp": "^2.1.0"
  }
}
